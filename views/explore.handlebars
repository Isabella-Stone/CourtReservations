<main>
    <h2>Explore</h2>
    {{#each users}}
        <div class="court-card">
            <p>{{this.firstName}} {{this.lastName}}</p>
            <p>{{this.username}}</p>
            <p>{{this.experience_level}}</p>
            <p>Rating: {{this.overallRating}}</p>
        </div>
    {{/each}}
</main>